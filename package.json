{
  "name": "FindMyStuff",
  "displayName": "FindMyStuff",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "e2e": "detox test e2e --configuration ios.sim.debug --cleanup",
    "e2e:build": "detox build-framework-cache",
    "build:ios": "react-native run-ios --simulator='iPhone 14'",
    "build:ios:release": "react-native run-ios --mode Release --simulator='iPhone 14'",
    "build:android": "react-native run-android"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^1.9.7",
    "date-fns": "^2.30.0",
    "react": "18.2.0",
    "react-native": "^0.73.0",
    "react-native-fs": "^2.20.0",
    "react-native-image-crop-picker": "^0.40.0",
    "react-native-image-resizer": "^1.4.4",
    "react-native-paper": "^5.11.0",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.27.0",
    "react-native-sqlite-storage": "^6.0.0",
    "react-native-vector-icons": "^10.0.0",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@react-native/metro-config": "^0.82.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^29.5.8",
    "@types/react": "18.2.37",
    "@types/react-native": "0.73.0",
    "@types/uuid": "^9.0.7",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.0.0",
    "detox-cli": "^20.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.2.2"
  }
}
